package com.lec.test;

public class Product {
	private String name;
	private int price;
	private int ps;
	
	public Product () {}

	public Product(String name, int price, int ps) {
		super();
		this.name = name;
		this.price = price;
		this.ps = ps;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getPs() {
		return ps;
	}

	public void setPs(int ps) {
		this.ps = ps;
	}
	
}
