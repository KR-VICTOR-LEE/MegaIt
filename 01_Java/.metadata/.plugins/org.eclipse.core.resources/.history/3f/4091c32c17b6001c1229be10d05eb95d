package com.lec.ex1_awt;

import java.awt.*;

public class Ex03 extends Frame {
	private Panel panel;
	private TextField txtfield;
	private Button btnOk, btnExit;
	private List list;
	
	public Ex03() {
		panel= new Panel();
		txtfield = new TextField();
		btnOk = new Button();
		btnExit = new  Button();
		list = new List();
		panel.add(new Label("write"));
		panel.add(txtfield);
		
		
	}
	
	public Ex03(String title) {
		this();
		setTitle(title);
	}
}
