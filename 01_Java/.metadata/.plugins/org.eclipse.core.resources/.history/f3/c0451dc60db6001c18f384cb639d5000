package com.lec.ex1_awt;

import java.awt.*;
	
public class Ex02Login extends Frame{
	private Label lbl1;
	private TextField txtId;
	private Label lbl2;
	private TextField txtPw;
	private Button btnLogin;
	
	public Ex02Login(Label lbl1, TextField txtId, Label lbl2, TextField txtPw, Button btnLogin)
			throws HeadlessException {
		super();
		this.lbl1 = lbl1;
		this.txtId = txtId;
		this.lbl2 = lbl2;
		this.txtPw = txtPw;
		this.btnLogin = btnLogin;
	}
	
}
