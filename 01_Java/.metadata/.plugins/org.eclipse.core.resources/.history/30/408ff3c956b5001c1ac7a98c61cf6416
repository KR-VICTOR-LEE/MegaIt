package com.lec.ex4_threadNObjectN;

public class ThreadEx extends Thread{
	private int num = 0;

	public ThreadEx(String name) {
		super(name);
	}
	
	@Override
	public void run() {
		for (int i = 0; i <10; i++) {
			System.out.println("¼öÇàÁß");
			num++;
		}
		System.out.println(Thread.currentThread().getName());
		try {
			Thread.sleep(5000);
		} catch (InterruptedException e) {}
	}

	public int getNum() {
		return num;
	}
	
}
