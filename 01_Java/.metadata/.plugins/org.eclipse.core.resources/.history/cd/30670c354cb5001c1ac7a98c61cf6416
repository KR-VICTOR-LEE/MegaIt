	public static void main(String[] args) {
		Runnable target1 = new TargetEx01();
		Runnable target2 = new TargetEx02();
		
		// "A"라는 이름의 쓰레드 생성
		Thread threadA = new Thread(target1, "A");
		Thread threadB = new Thread(target2, "B");

	}

}
