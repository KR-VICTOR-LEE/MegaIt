package com.lec.ex4_threadNObjectN;

public class ThreadEx extends Thread {
	private int num = 0;

	public ThreadEx(String name) {
		super(name);
	}

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("~ ~ ~ A 쓰레드 수행중 ~ ~ ~");
			num++;
		}
		try {
			Thread.sleep(500);
		} catch (InterruptedException e) {
		}
	}

	private void out() {

	}

	public int getNum() {
		return num;
	}

}
