package com.lec.ex3_set;

public class Student {
	private int grade;
	private String name;

	public Student(int grade, String name) {
		this.grade = grade;
		this.name = name;
	}

	@Override
	public String toString() {
		return grade + "ÇÐ³â " + name;
	}
	// equals ¿Í hashcode override 
	@Override
	public int hashCode() {
		return toString().hashCode();
	}

}
